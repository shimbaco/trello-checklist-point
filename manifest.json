{
  "manifest_version": 2,
  "name": "Trello Checklist Point",
  "version": "1.0.1",
  "description": "Trelloのチェックリストに追加したポイントを足し算するエクステンション",
  "icons": {
    "128": "images/icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://trello.com/*"],
      "css": ["stylesheets/application.css"],
      "js": [
        "javascripts/vendor/jquery-2.2.1.min.js",
        "javascripts/vendor/lodash-4.5.1.min.js",
        "javascripts/application.js"
      ]
    }
  ]
}
